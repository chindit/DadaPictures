{
    "name": "chindit/dadapictures",
    "description": "A small project to upload packs of pictures",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "gedmo/doctrine-extensions": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.4",
        "league/flysystem-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/asset": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/finder": "5.2.*",
        "symfony/flex": "^1.12",
        "symfony/form": "^5.2",
        "symfony/mailer": "5.2.3",
        "symfony/messenger": "5.2.3",
        "symfony/monolog-bundle": "^3.6",
        "symfony/orm-pack": "^2.1",
        "symfony/polyfill-apcu": "^1.22",
        "symfony/security-bundle": "^5.2",
        "symfony/translation": "5.2.3",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/webpack-encore-bundle": "^1.9",
        "symfony/yaml": "^5.2",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.28",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
