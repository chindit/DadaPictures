{
    "name": "chindit/dadapictures",
    "description": "A small project to upload packs of pictures",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "gedmo/doctrine-extensions": "^3.3",
        "knplabs/knp-paginator-bundle": "^5.7",
        "league/flysystem-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.2",
        "symfony/asset": "^5.3",
        "symfony/doctrine-messenger": "5.3.10",
        "symfony/dotenv": "^5.3",
        "symfony/filesystem": "5.3.4",
        "symfony/finder": "5.3.7",
        "symfony/flex": "^1.17",
        "symfony/form": "^5.3",
        "symfony/mailer": "5.3.9",
        "symfony/messenger": "5.3.11",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-apcu": "^1.23",
        "symfony/proxy-manager-bridge": "5.3.11",
        "symfony/security-bundle": "^5.3",
        "symfony/translation": "5.3.11",
        "symfony/twig-bundle": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "^5.3",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/debug-bundle": "^5.3",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.3"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
